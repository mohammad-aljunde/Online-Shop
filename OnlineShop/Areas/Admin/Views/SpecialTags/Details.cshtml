@model SpecialTags


<br />
<h2 class="text-info">Details Special Tags</h2>


<form asp-action="Index">
    <div class="p-4 rounded border">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="SpecialTag"></label>
            </div>
            <div class="col-5">
                <input asp-for="SpecialTag" class="form-control" readonly />
            </div>
            <span asp-validation-for="SpecialTag" class="text-danger"></span>
        </div>
        <div class="form-group">

            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
            
            <input type="submit" class="btn btn-success" value="Back To List" />
        </div>
    </div>
</form>


